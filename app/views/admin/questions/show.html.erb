<%- model_class = Question -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:question) %>:</strong></dt>
  <dd><%= @question.question %></dd>
  <dt><strong><%= model_class.human_attribute_name(:q_type) %>:</strong></dt>
  <dd><%= @question.q_type %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              admin_questions_path, :class => 'btn btn-default'  %>

